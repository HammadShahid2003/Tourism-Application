{
  "projects": [
    {
      "name": "frontend",
      "src": "frontend",
      "use": "@vercel/next"
    },
    {
      "name": "backend",
      "src": "backend/api",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
